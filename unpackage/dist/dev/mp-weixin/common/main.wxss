	padding-right: 40rpx;
}
.feedback-quick:after {
	font-family: uniicons;
	font-size: 40rpx;
	content: '\e581';
	position: absolute;
	right: 0;
	top: 50%;
	color: #bbb;
	-webkit-transform: translateY(-50%);
